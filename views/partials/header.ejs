<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Interview-Tracker</title>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
  <link rel="stylesheet" href="/styles.css">
</head>
<body>
  <nav>
    <h1><a href="/">Interview Tracker</a></h1>
    <ul>
      <li><a href="/">Home</a></li>
      <% if(user) { %>
      <!-- <li><a href="/userUpdation" class="button"></a>,<%= user.name %></li>
      <li><a href="/logout">Log-Out</a></li> -->
      <% if (user.name){ %>
        <li><a href="/userUpdation" class="button">Hi <%= user.name%></a></li>
        <li><a href="/logout">Log out</a></li>
        <% } else {%>
          <li><a href="/userUpdation" class="button">Hi <%= user.email%></a></li>
          <li><a href="/logout">Log out</a></li>
      <% } } else { %>
      <li><a href="/login">Log-In</a></li>
      <li><a href="/signup" class="btn">Sign-Up</a></li>
      <% } %> 
    </ul>
  </nav>
  